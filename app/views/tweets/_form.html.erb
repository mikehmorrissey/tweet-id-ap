<%= form_for (@tweet) do |f|%>

	<% if @tweet.errors.any?%>
		<% @tweet.errors.full_messages.each do |mes| %>
			<div class="error">* <%=mes%></div>
		<% end %>
	<%end%>


	<div class= "field" >
		
		<%= f.number_field :tweet_id %>
	</div>

	<div class = "submit">
		<%= f.submit "Find Tweet", class: "submitbutton"%>
	</div>
<% end %>